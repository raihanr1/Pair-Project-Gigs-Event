<%- include('./partials/header') %>
  <%- include('./partials/navbar') %>

    <div class="container-md mt-3">
      <h1 class="text-center"><strong>Profile</strong></h1>
      <br>

      <div class="row">
        <p class="fs-4">Welcome to Gig's Events!! Music junkies,</p><br>
      </div>

      <div class="row">
        <p class="fs-4"><strong>Name :</strong></p>
        <p class="fs-4">
          <%=data[0].firstName%>
            <%=data[0].lastName %>
        </p>
        <br>
      </div>

      <div class="row">
        <p class="fs-4"><strong>Birth Date :</strong></p>
        <p class="fs-4">
          <%=data[0].formatDate%>
        </p>
        <br>
      </div>

      <div class="row">
        <p class="fs-4"><strong>Favorite Genre :</strong></p>
        <p class="fs-4">
          <%=data[0].favoriteGenre%>
        </p>
        <br>
      </div>

      <button type="submit" class="btn btn-dark mt-1 mb-5"><a href="/user/profile/edit"
          style="text-decoration: none; color:white">Edit</a></button>
    </div>


    <%- include('./partials/footer') %>